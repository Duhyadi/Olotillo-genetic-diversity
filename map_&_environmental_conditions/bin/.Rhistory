mexico <- st_read(here("shape", "destdv1gw.shp"))        #  shape of Mexico
distribution <- st_read(here("shape", "oloti_7cgw.shp")) # shape of distribution
olotillo.PGMN <- read.csv(here("meta", "olotillo_PGMN_siagro.csv")) # points PGMN
